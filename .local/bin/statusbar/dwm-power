#!/bin/sh

power() {
	read -r power_now0 < /sys/class/power_supply/BAT0/power_now
	read -r power_now1 < /sys/class/power_supply/BAT1/power_now
	power0=$((power_now0/1000000))
	power1=$((power_now1/1000000))
	echo "ïƒ§ Power drain = Internal battery : $power0 W - External battery : $power1 W"
}

echo $(power)
